        "cookie": "^0.4.1"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/kit": "next",
        "@types/cookie": "^0.4.1",
        "autoprefixer": "^10.4.2",
        "postcss": "^8.4.7",
        "prettier": "^2.5.1",
        "prettier-plugin-tailwindcss": "^0.1.8",
        "svelte": "^3.46.0",
        "svelte-check": "^2.2.6",
        "svelte-preprocess": "^4.10.1",
        "tailwindcss": "^3.0.23",
        "tslib": "^2.3.1",
        "typescript": "~4.5.4"
    }
